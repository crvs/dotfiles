XTerm*termName: xterm-256color
XTerm*utf8:1
XTerm*locale: true
!XTerm*font: 8x13
!XTerm*font: 7x13
!XTerm*reverseVideo:on
!xterm*faceName: Dejavu Sans Mono:antialias=true:size=10
!xterm*faceName: Adobe Source Code:antialias=false:size=9
!xterm*font: -*-terminus-medium-*-*-*-18-*-*-*-*-*-iso10646-*
!XTerm*foceName: "Liberation Mono:size=8:antialias=true"
!XTerm*faceName: monofur:pixelsize=15
!XTerm*faceSize: 9

dzen2.font:       xft:monofur for Powerline-10.5:regular
dzen2.foreground: #22EE11
dzen2.background: black

! These used to be XTerm*
*pointerColor: white
*pointerColorBackground: black
*cursorColor:yellow
*cursorBlink:true

Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

URxvt*font:     xft:monofur for Powerline-10.5:regular
URxvt*faceName: xft:monofur for Powerline-10.5:regular
*fullscreen:never

XTerm*urgentOnBell:true

